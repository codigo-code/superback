<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>examen</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>informacion</servlet-name>
    <servlet-class>com.utn.controlador.servletinformacion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>informacion</servlet-name>
    <url-pattern>/informacion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>validaroperacion</servlet-name>
    <servlet-class>com.utn.controlador.servletvalidaroperacion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>validaroperacion</servlet-name>
    <url-pattern>/validaroperacion</url-pattern>
  </servlet-mapping>
</web-app>